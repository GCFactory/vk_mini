{
  "name": "refactor.loc",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "eslint": "^6.6.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.7.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^5.0.0",
    "react-router5": "^7.1.0",
    "react-scripts": "^3.0.1",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2",
    "router5-plugin-logger": "^7.0.2",
    "seamless-immutable": "^7.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/serviceWorker.js",
      "!src/index.js",
      "!node_modules/",
      "!public/",
      "!build/",
      "!coverage/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
